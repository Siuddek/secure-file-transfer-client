<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>


<?import javafx.scene.control.ToolBar?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.bsk.Controllers.FileChooserController"
            prefHeight="400.0" prefWidth="600.0">

    <ToolBar prefHeight="80.0" prefWidth="800">
        <Pane HBox.hgrow="ALWAYS"/>
        <Button fx:id="fileChooser" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false"
                onAction="#chooseAndEncryptFile" text="Send encrypted file" textAlignment="CENTER" StackPane.alignment="TOP_CENTER"/>
    </ToolBar>

</AnchorPane>
